source 'https://github.com/CocoaPods/Specs.git'
platform :ios, ‘9.0’

def shared_pods
  use_frameworks!
  inhibit_all_warnings!

  pod 'Alamofire', '~> 4.4.0'
  pod 'SwiftyJSON', '~> 3.1.4'
  pod 'SVProgressHUD', '~> 2.1.2'
  pod 'SDWebImage', '~> 4.0.0'
end

target ‘LegalCalculator’ do
    shared_pods
end